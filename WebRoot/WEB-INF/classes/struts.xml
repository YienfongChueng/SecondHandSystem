<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
	<struts>
		 <package name="ssh" extends="struts-default" namespace="/">
			 <action name="userLogin" class="loginAction" method="UserLogin">
				<result name="index" type="redirect">/index.jsp</result>
				<result name="login">/user_login.jsp</result>
			 </action> 
			 <action name="adminLogin" class="adminLoginAction" method="AdminLogin">
				<result name="adminIndex" type="redirect">/index.jsp</result>
				<result name="adminLogin">/admin_login.jsp</result>
			 </action>
			
		</package> 
	</struts>