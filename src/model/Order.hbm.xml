<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
    <class name="model.Order" table="tb_order">
    	<id name="id" column="id" type="java.lang.Integer" length="8">
    		<generator class="native"/>
    	</id>
    	<property name="createTime" column="CREATE_TIME" type="java.util.Date"/>	
    	<property name="status" column="STATUS" type="java.lang.Integer" length="8"/>
    	<property name="userId" column="USER_ID" type="java.lang.Integer" length="8"/>
    	<property name="personName" column="PERSON_NAME" type="java.lang.String" length="50"/>
    	<property name="phone" column="PHONE" type="java.lang.Integer" length="20"/>
    	<property name="amount" column="AMOUNT" type="java.lang.Double" length="16"/>
    	<property name="address" column="ADDRESS" type="java.lang.String" length="50"/>	
    	
    </class>
    </hibernate-mapping>